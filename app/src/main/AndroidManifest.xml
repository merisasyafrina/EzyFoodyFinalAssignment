<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hfad.ezyfoody">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MapsActivity"></activity>
        <activity android:name=".History.HistoryList" />
        <activity android:name=".Activities.DetailHistoryActivity" />
        <activity android:name=".Activities.HistoryActivity" />
        <activity android:name=".Activities.MenuActivity.SnacksActivity" />
        <activity android:name=".Activities.MenuActivity.FoodActivity" />
        <activity android:name=".Activities.TopUpActivity" />
        <activity android:name=".Menu.MenuList" />
        <activity android:name=".CompleteOrder.CompleteOrderList" />
        <activity android:name=".Activities.CompleteOrderActivity" />
        <activity android:name=".Activities.OrderActivity" />
        <activity android:name=".Activities.MyOrderActivity" />
        <activity android:name=".Activities.MenuActivity.DrinksActivity" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="${mapsApiKey}" />

</manifest>